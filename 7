#include <stdio.h>
#include <stdlib.h>
int n;
int Fib(int i)
{
  if(i < 1) return 0;
  if(i == 1) return 1;
  return Fib(i-1) + Fib(i - 2);
}
int input()
{
  printf("How numbers to display?\n");
  scanf("%d",&n);
  return n;
}
int main()
{
  int i;
  input();
  for (i=1; i<=n; i++)
{
  printf ("%d ", Fib(i)); 
}
return 0;
}
