#include <iostream> 
using namespace std; 
     int Fib(int i)  
{
     if(i < 1) return 0;
     if(i == 1) return 1;
     return Fib(i-1) + Fib(i - 2); 
}
     int main()
{     
     char *input = "How many numbers to display?\n";
     printf("%s", input);
     {
     int i;
     int n; 
     scanf("%d",&n);
     for (i=1; i<=n; i++)
     {
     printf ("%d ", Fib(i)); 
     }
     return 0;
     }
}
