#include <stdio.h>
#include <stdlib.h> 
     int input()
{
     printf("How numbers to display?\n");
}
     int main()
{
     input();
}
     int Fib(int i)  
{
     if(i < 1) return 0;
     if(i == 1) return 1;
     return Fib(i-1) + Fib(i - 2); 
}

{
     int i;
     int n; 
     scanf("%d",&n);
     for (i=1; i<=n; i++)
     {
     printf ("%d ", Fib(i)); 
     }
     return 0;
}
